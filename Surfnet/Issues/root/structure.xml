<?xml version="1.0" encoding="UTF-8"?>
<CredentialStructure xmlns="http://www.zurich.ibm.com/security/idemix"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
    xmlns:xs="http://www.w3.org/2001/XMLSchema"
    xsi:schemaLocation="http://www.zurich.ibm.com/security/idemix CredentialStructure.xsd">
    
    <Attributes>
        <Attribute issuanceMode="known" name="userID" type="string" />
        <Attribute issuanceMode="known" name="securityHash" type="int" />
    </Attributes>

    <Features/>

    <Implementation>
        <AttributeOrder>
            <Attribute name="userID">1</Attribute>
            <Attribute name="securityHash">2</Attribute>
        </AttributeOrder>
    </Implementation>

</CredentialStructure>